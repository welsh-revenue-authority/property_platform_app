  "license": "ISC",
  "bugs": {
    "url": "https://github.com/welsh-revenue-authority/property_platform_app/issues"
  },
  "dependencies": {
    "wra-frontend": "https://gitpkg.now.sh/welsh-revenue-authority/wra-frontend/package?main"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "standardx": "^7.0.0"
  }
}
