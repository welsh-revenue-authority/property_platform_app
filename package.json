{
  "name": "property-platform-app",
  "version": "0.0.1",
  "description": "A poc property data platform",
  "scripts": {
    "build:stylesheets": "npm run nps build.stylesheets",
    "build:gel": "npm run nps build.gel",
    "watch:scss": "npm run nps watch.stylesheets",
    "watch": "npm-run-all --parallel watch:*",
    "nps": "nps -c ./node_modules/wra-frontend/package-scripts.js",
    "postinstall": "npm run nps build.gel && npm run nps build.stylesheets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welsh-revenue-authority/property_platform_app"
  },
